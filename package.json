    "socket.io-client": "^4.7.2",
    "webpack": "5.89.0"
  },
  "peerDependencies": {
    "@babel/compat-data": "~7.19.4",
    "@babel/core": "~7.19.6",
    "@babel/generator": "~7.19.6",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@dugrema/node-forge": "https://github.com/dugrema/forge#ed25519",
    "multiformats": "^9"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gh-pages": "^6.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "react-dom": "^18",
    "react-full-screen": "^1.1.1",
    "react-scripts": "^5"
