{
  "name": "millegrilles.reactjs",
  "version": "2023.11.4",
  "description": "Composants communs pour MilleGrilles front-end avec ReactJS",
  "author": "dugrema",
  "license": "MIT",
  "repository": "dugrema/millegrilles.reactjs",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "@dugrema/millegrilles.utiljs": "https://github.com/dugrema/millegrilles.utiljs.git#2023.11",
    "@react-hooks-hub/use-media-query": "^1.1.5",
    "@reduxjs/toolkit": "^1.9.7",
    "@zip.js/zip.js": "^2.7.30",
    "axios": "^1.6.0",
    "base64url": "^3.0.1",
    "bootstrap": "^5.3",
    "comlink": "^4.4.1",
    "font-awesome": "^4.7.0",
    "hash-wasm": "^4.10.0",
    "html5-qrcode": "^2.3.8",
    "i18n": "^0.15.1",
    "idb": "^7.1.1",
    "libsodium-wrappers": "^0.7.13",
    "moment-duration-format": "^2.3",
    "moment-timezone": "^0.5.43",
    "multibase": "^4.0.6",
    "pako": "^2.1.0",
    "react": "^18",
    "react-bootstrap": "^2.9",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^13.3.1",
    "react-redux": "^8.1.3",
